{
    "extends": "airbnb",
    "parserOptions": {
        "sourceType": "script"
    },
    "rules": {
        "indent": [2, 4],
        "func-names": 0,
        "no-param-reassign": 0,
        "space-before-function-paren": [2, "always"],
        "strict": [2, "global"],
        "arrow-body-style": 0,
        "global-require": 0
    },
    "globals": {
        "log": true
    }
}
