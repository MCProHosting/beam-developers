{
    "Authenticate": {
        "description": "Authenticate to join as an active user in a specified channel",
        "example": {
            "request": {
                "type": "method",
                "method": "auth",
                "arguments": ["CHANNELID", "USERID", "32hexadedimalAuthKey"],
                "id": 0
            },
            "response": {
                "type": "reply",
                "error": null,
                "id": 1,
                "data": {
                    "authenticated": true,
                    "role": "Owner"
                }
            }
        }
    },
    "Send message": {
        "description": "Send a chat message to the server, the server will not directly reply unless an error occurred. For confirmation use the events.",
        "example": {
            "request": {
                "type": "method",
                "method": "msg",
                "arguments": ["Hello World!"],
                "id": 2
            }
        }
    },
    "Voting": {
        "description": "Casts a vote for the current poll, `arguments` is the vote option index.",
        "example": {
            "request": {
                "type": "method",
                "method": "vote",
                "arguments": [0],
                "id": 3
            },
            "response": {
                "type": "reply",
                "error": null,
                "id": 3,
                "data": true
            }
        }
    }
}
